body{font-size:14px;line-height: 150%;}
.gatewayTop{height:32px;line-height:32px;background:#0f97e9;color:#fff;font-size:13px;}
.gatewaySearchSel{background-color: #0e7ab7;border: none;color: #f1f1f1}
.gatewaySearchTxt{position: relative; background-color: #0e7ab7;border: none;color: #f1f1f1;}
.gatewaySearchBtn{background-color: #0e7ab7;border: none;}
.gatewaySearchBtn span{color:#fff;}
.gatewayLogin{}
.gatewayLoginSelect{background:#3BACDE;color:#fff;vertical-align: middle;padding:0;margin-top:-6px;border: 0 none;}
.gatewayTop a{color:#fff;vertical-align: middle;}
.gatewayTop a:hover{background:0 none;}
.gateway-navbar-customer {
    background: #f9f9f9;
    border-bottom: 1px solid #E1D5D5;
    box-shadow: #cecece 0 2px 8px;
    z-index: 2;
    height: 63px;
    opacity: 0.7;
}
.navbar-nav>li{text-align: center;}
.navbar-nav>li > a {
    font-size: 17px;
    color: black;
}
.pipe {
    font-weight: 100;
    font-size: 24px;
    color: rgba(255,255,255,0.3);
    text-shadow: 1px 0 0.2px rgba(0,0,0,0.1);
    padding-top: 12px;
    width: 0;
    margin-right:5px;
}

.logo {
    /* background-image: url('../img/logo.png'); */
    /* background-repeat: no-repeat; */
    padding: 0;
    width: 130px;
}
.navbar-nav a:hover{background:0 none;}
.logo{margin-right:25px;}
.gatewayMain{background:#f7f7f7;}
