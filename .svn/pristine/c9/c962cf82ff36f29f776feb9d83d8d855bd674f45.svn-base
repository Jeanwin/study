package com.zonekey.study.vo;

import com.zonekey.study.entity.CollectionRecord;

public class CollectionRecordView extends CollectionRecord {

	private static final long serialVersionUID = 3379745489393408170L;

	private String courseName;
	private long timeLength;
	private String watchNum;
	private String commentNum;
	private String userName;
	private String deptName;
	private String picture;
	private String playRecord;

	public String getCourseName() {
		return courseName;
	}

	public void setCourseName(String courseName) {
		this.courseName = courseName;
	}

	public long getTimeLength() {
		return timeLength;
	}

	public void setTimeLength(long timeLength) {
		this.timeLength = timeLength;
	}

	public String getWatchNum() {
		return watchNum;
	}

	public void setWatchNum(String watchNum) {
		this.watchNum = watchNum;
	}

	public String getCommentNum() {
		return commentNum;
	}

	public void setCommentNum(String commentNum) {
		this.commentNum = commentNum;
	}

	public String getUserName() {
		return userName;
	}

	public void setUserName(String userName) {
		this.userName = userName;
	}

	public String getPicture() {
		return picture;
	}

	public void setPicture(String picture) {
		this.picture = picture;
	}

	public String getPlayRecord() {
		return playRecord;
	}

	public void setPlayRecord(String playRecord) {
		this.playRecord = playRecord;
	}

	public String getDeptName() {
		return deptName;
	}

	public void setDeptName(String deptName) {
		this.deptName = deptName;
	}

	@Override
	public String toString() {
		return "CollectionRecordView [courseName=" + courseName + ", timeLength=" + timeLength + ", watchNum=" + watchNum + ", commentNum=" + commentNum + ", userName=" + userName + ", deptName="
				+ deptName + ", picture=" + picture + ", playRecord=" + playRecord + ", toString()=" + super.toString() + "]";
	}

}
