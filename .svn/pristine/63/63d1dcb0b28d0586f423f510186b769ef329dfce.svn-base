package com.zonekey.study.vo;

import com.zonekey.study.entity.CollectionRecord;

public class CollectionRecordView extends CollectionRecord {

	private static final long serialVersionUID = 3379745489393408170L;

	private String courseName;
	private long duration;
	private int viewNum;
	private int commentNum;
	private String username;
	private String deptName;
	private String picture;
	private long playtime;
	private String watchtime;
	private String curriculumid;

	public String getCourseName() {
		return courseName;
	}

	public void setCourseName(String courseName) {
		this.courseName = courseName;
	}

	public long getDuration() {
		return duration;
	}

	public void setDuration(long duration) {
		this.duration = duration;
	}

	public long getPlaytime() {
		return playtime;
	}

	public void setPlaytime(long playtime) {
		this.playtime = playtime;
	}

	public int getViewNum() {
		return viewNum;
	}

	public void setViewNum(int viewNum) {
		this.viewNum = viewNum;
	}

	public int getCommentNum() {
		return commentNum;
	}

	public void setCommentNum(int commentNum) {
		this.commentNum = commentNum;
	}

	public String getUsername() {
		return username;
	}

	public void setUsername(String username) {
		this.username = username;
	}

	public String getPicture() {
		return picture;
	}

	public void setPicture(String picture) {
		this.picture = picture;
	}

	public String getDeptName() {
		return deptName;
	}

	public void setDeptName(String deptName) {
		this.deptName = deptName;
	}

	public String getCurriculumid() {
		return curriculumid;
	}

	public void setCurriculumid(String curriculumid) {
		this.curriculumid = curriculumid;
	}

	public String getWatchtime() {
		return watchtime;
	}

	public void setWatchtime(String watchtime) {
		this.watchtime = watchtime;
	}

	@Override
	public String toString() {
		return "CollectionRecordView [courseName=" + courseName + ", duration=" + duration + ", viewNum=" + viewNum + ", commentNum=" + commentNum + ", username=" + username + ", deptName="
				+ deptName + ", picture=" + picture + ", playtime=" + playtime + ", watchtime=" + watchtime + ", curriculumid=" + curriculumid + "]";
	}

}
