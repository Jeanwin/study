var timeFmt=function(s){
			var h=Math.floor(s/3600);
			h=h>0?(h>9?h:'0'+h)+':':'';
			var m=Math.floor(s/60)%60;
			m=(m>9?m:'0'+m)+':';
			var S=Math.floor(s%60);
			S=S>9?S:'0'+S;
			return h+m+S;	
},
