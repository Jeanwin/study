.selectedList {
	position: absolute;
	right: 0;
	top: 0;
	width: 80%;
	height: 100%;
	z-index: 1000;
	color: #fff;
	padding: 3%;
}

.selectedList span {
	line-height: 30px;
}

.selectedList ul li {
	width: 100%;
