html {
	font-size: 12px;
}

body {
	font-size: 12px;
	font-family: "Microsoft YaHei";
}

header {
	position: fixed;
	top: 0;
	left: 0;
	width: 100%;
	height: 50px;
	line-height: 50px;
	border-bottom: 1px #414042 solid;
	background-color: #fff;
	text-align: center;
	font-size: 14px;
	font-weight: bold;
	z-index: 998;
	color: #666;
}

.logo {
	font-size: 2rem;
	position: absolute;
	left: 10px;
	top: 0;
	color: #005aa9;
	font-weight: bold;
}

.back {
	font-size: 2.5rem;
	position: absolute;
	left: 10px;
	top: 0;
}

.back a {
	color: #666;
	font-weight: bold;
}

.iconBar {
	font-size: 2rem;
	position: absolute;
	right: 10px;
	top: 13px;
	height: 24px;
	line-height: 24px;
	color: #666666;
	font-weight: bold;
}

.iconCol {
	position: absolute;
	right: 10px;
	top: 13px;
	height: 24px;
	line-height: 24px;
	color: #666666;
}

.iconBar a {
	color: #666666;
	font-weight: bold;
	font-size: 2rem;
}

.iconCol span {
	font-size: 12px;
	color: #f26522;
	font-weight: normal;
}

@media screen and (min-width: 450px) {
	nav {
		position: absolute;
		top: 50px;
		left: 0;
		z-index: 999;
		width: 100%;
		height: 40px;
		line-height: 40px;
		opacity: 0.8;
		background-color: #000;
	}
	nav a {
		display: inline-block;
		width: 15%;
		height: 24px;
		line-height: 24px;
		-moz-border-radius: 3px;
		-webkit-border-radius: 3px;
		border-radius: 3px;
		background-color: #fff;
		color: #333;
		text-align: center;
		margin: 0 2%;
	}
}

