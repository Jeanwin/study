.container {
	padding-top: 50px;
}

.focusImage {
	width: 100%;
	height: 60%;
}

.focusImage img {
	width: 100%;
	height: 60%;
}

.infoTip {
	background: #d4eef9;
	height: 30px;
	line-height: 30px;
	padding: 0 10px;
}

.infoTip h3 {
	font-size: 12px;
	color: #6a777c;
	width: 100%;
	height: 30px;
	line-height: 30px;
	white-space: nowrap;
	overflow: hidden;
	text-overflow: ellipsis;
}

.infoTip a {
	float: right;
	color: #77b0da;
}

.mainContent {
	width: 100%; /*background-color:#eaeced;*/
}

.mainContent .tabTitle {
	width: 100%;
	height: 40px;
	line-height: 40px;
	background-color: #fff;
	border-bottom: 1px #bec0c2 solid;
}

.mainContent .tabTitle span {
	width: 49%;
	display: inline-block;
	text-align: center;
	font-size: 14px;
}

.mainContent .tabTitle span.current {
	border-bottom: 2px #1c75bc solid;
}

.mainContent .tabTitle span a {
	color: #333;
}

.contentLive {
	
}

.liveInfo {
	padding: 8px;
	width: 96%;
	height: 80px;
	margin: 10px auto;
	border: 1px #bec0c2 solid;
	background-color: #fff;
	position: relative;
}

.liveInfo .liveTitle {
	display: inline-block;
	width: 100%;
	height: 30px;
	line-height: 30px;
	font-size: 12px;
	border-bottom: 1px #e2e2e3 solid;
	color: #393637;
	white-space: nowrap;
	overflow: hidden;
	text-overflow: ellipsis;
}

.liveInfo .liveTitle span {
	color: #1c75bc;
	display: inline;
}

.liveInfo .liveTitle h3 {
	display: inline;
	width: 100%;
	white-space: nowrap;
	overflow: hidden;
	text-overflow: ellipsis;
}

.liveInfo .time {
	position: absolute;
	top: 0;
	right: 0;
	background-color: #918f8f;
	padding: 3px 10px;
	color: #fff;
}

.liveInfoCont {
	font-size: 12px;
	position: relative;
}

.liveInfoCont i {
	font-size: 14px;
	margin-right: 3px;
	font-weight: 700;
	vertical-align: middle;
}

.liveInfoAuthor {
	position: absolute;
	left: 0;
	top: 10px;
	color: #7b7979;
}

.liveInfoAuthor span {
	vertical-align: middle;
}

.liveInfoAuthor i {
	vertical-align: middle;
}

.liveInfoOpt {
	position: absolute;
	right: 0;
	top: 10px;
	color: #9d9d9f;
}

.liveInfoOpt span {
	margin-left: 15px;
}

.contentLive .more {
	width: 100%;
	padding-bottom: 10px;
	text-align: center;
}

.contentLive .more a {
	display: block;
	color: #767777;
}

.mlIcon {
	margin-left: 8px;
}

/*******直播点播列表页样式*******/
.search {
	position: absolute;
	top: 50px;
	left: 0;
	width: 100%;
	height: 40px;
	line-height: 40px;
	color: #333;
	font-size: 12px;
	background-color: #1c75bc;
	z-index: 998;
}

.search input {
	width: 80%;
	height: 30px;
	-moz-border-radius: 5px;
	-webkit-border-radius: 5px;
	border-radius: 5px;
	border: 0 none;
	font-weight: normal;
	padding: 9px 10px;
}

.search button {
	height: 40px;
	line-height: 40px;
	color: #fff;
	overflow: hidden;
	background: none;
	border: 0 none;
	outline: none;
	font-size: 12px;
	margin-left: 5px;
}

.search button i {
	font-size: 14px;
}

.playingState {
	background-color: #f6f8fa;
}

.recentWatch .title {
	height: 30px;
	line-height: 30px;
	background-color: #e0e7ed;
	padding-left: 5px;
	font-size: 12px;
}

.recentWatch .title i {
	font-size: 14px;
	margin: 0 5px;
}

.playList {
	padding: 5px 10px 0;
}

.playList>a {
	display: block;
	color: #313232;
	border-bottom: 1px #e2e2e3 solid;
	padding: 5px;
}

.playList h3 {
	width: 100%;
	white-space: nowrap;
	overflow: hidden;
	text-overflow: ellipsis;
}

.playList .resentLive {
	position: relative;
	height: 30px;
	line-height: 30px;
}

.playList .liveAuthor {
	position: absolute;
	top: 0;
	left: 0;
	color: #7b7979;
}

.playList .liveAuthor i {
	font-size: 16px;
}

.paddingTop90 {
	padding-top: 90px;
}

.paddingTop40 {
	padding-top: 40px;
}

.paddingTop50 {
	padding-top: 50px;
}

